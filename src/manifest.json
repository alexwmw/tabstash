{
  "manifest_version": 3,
  "version": "1.2.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabGroups",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/main.png"
  }
}
